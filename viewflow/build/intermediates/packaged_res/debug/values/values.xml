<?xml version="1.0" encoding="utf-8"?>
<resources>
    <string name="app_name">ViewFlow</string>
    <string name="hello">Hello World!</string>
    <declare-styleable name="CircleFlowIndicator">
        <attr format="color" name="activeColor"/>
        <attr format="color" name="inactiveColor"/>
        <attr format="dimension" name="radius"/>
        <attr format="dimension" name="spacing"/>
		<attr format="boolean" name="centered"/>
		<attr format="integer" name="fadeOut"/>
		<attr name="inactiveType">
			<flag name="stroke" value="0"/>
			<flag name="fill" value="1"/>
		</attr>
		<attr name="activeType">
			<flag name="stroke" value="0"/>
			<flag name="fill" value="1"/>
		</attr>
		<attr format="boolean" name="snap"/>
    </declare-styleable>
    <declare-styleable name="TitleFlowIndicator">
        <attr format="dimension" name="titlePadding"/>
        <!-- Left/right padding of not active view titles. -->
        <attr format="dimension" name="clipPadding"/>
        <attr format="color" name="selectedColor"/>
        <attr format="boolean" name="selectedBold"/>
        <attr format="dimension" name="selectedSize"/>
        <attr format="color" name="textColor"/>
        <attr format="dimension" name="textSize"/>
        <attr format="dimension" name="footerLineHeight"/>
        <attr format="color" name="footerColor"/>
        <attr format="dimension" name="footerTriangleHeight"/>
        <attr format="string" name="customTypeface"/>
    </declare-styleable>
    <declare-styleable name="ViewFlow">
        <attr format="integer" name="sidebuffer"/>
    </declare-styleable>
</resources>